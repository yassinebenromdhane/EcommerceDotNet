    <UserSecretsId>bd01fe81-dceb-48b2-b26f-b60064e8388c</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.1">
